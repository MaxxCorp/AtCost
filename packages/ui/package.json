{
    "name": "@ac/ui",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "svelte": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "svelte": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./components/*": {
            "types": "./src/components/*/index.ts",
            "svelte": "./src/components/*/index.ts",
            "default": "./src/components/*/index.ts"
        },
        "./components/DashboardCard.svelte": "./src/components/DashboardCard.svelte",
        "./components/LoadingSection.svelte": "./src/components/LoadingSection.svelte",
        "./components/ErrorSection.svelte": "./src/components/ErrorSection.svelte",
        "./components/EmptyState.svelte": "./src/components/EmptyState.svelte",
        "./components/EntityManager.svelte": "./src/components/EntityManager.svelte",
        "./components/TagInput.svelte": "./src/components/TagInput.svelte",
        "./components/AsyncButton.svelte": "./src/components/AsyncButton.svelte",
        "./components/BulkActionToolbar.svelte": "./src/components/BulkActionToolbar.svelte",
        "./utils": {
            "types": "./src/utils.ts",
            "default": "./src/utils.ts"
        }
    },
    "peerDependencies": {
        "svelte": "^5.0.0"
    },
    "dependencies": {
        "bits-ui": "^2.16.2",
        "clsx": "^2.1.1",
        "tailwind-merge": "^3.5.0",
        "tailwind-variants": "^3.2.2",
        "svelte-sonner": "^1.0.7",
        "@lucide/svelte": "^0.575.0"
    },
    "devDependencies": {
        "svelte": "^5.53.2",
        "typescript": "^5.9.3"
    }
}