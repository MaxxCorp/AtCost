{
    "name": "@ac/db",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./schema": {
            "types": "./src/schema/index.ts",
            "default": "./src/schema/index.ts"
        },
        "./schema/*": {
            "types": "./src/schema/*.ts",
            "default": "./src/schema/*.ts"
        }
    },
    "dependencies": {
        "drizzle-orm": "^0.45.1"
    },
    "devDependencies": {
        "typescript": "^5.7.3"
    }
}